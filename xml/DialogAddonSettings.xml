<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- addonsettings -->
	<defaultcontrol always="true">9</defaultcontrol>
	<onload condition="String.IsEmpty(Window(Home).Property(DialogVisibility)) + String.IsEmpty(Window(Home).Property(DialogVisibility2))">SetProperty(DialogVisibility,$INFO[Window.Property(xmlfile)]1,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility)) + String.IsEmpty(Window(Home).Property(DialogVisibility2))">SetProperty(DialogVisibility2,$INFO[Window.Property(xmlfile)]2,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.IsEmpty(Window(Home).Property(DialogVisibility3))">SetProperty(DialogVisibility3,$INFO[Window.Property(xmlfile)]3,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4))">SetProperty(DialogVisibility4,$INFO[Window.Property(xmlfile)]4,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))">SetProperty(DialogVisibility5,$INFO[Window.Property(xmlfile)]5,Home)</onload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.Contains(Window(Home).Property(DialogVisibility),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility),$NUMBER[1])">ClearProperty(DialogVisibility,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.Contains(Window(Home).Property(DialogVisibility2),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility2),$NUMBER[2])">ClearProperty(DialogVisibility2,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.Contains(Window(Home).Property(DialogVisibility3),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility3),$NUMBER[3])">ClearProperty(DialogVisibility3,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility5)) + String.Contains(Window(Home).Property(DialogVisibility4),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility4),$NUMBER[4])">ClearProperty(DialogVisibility4,Home)</onunload>
	<onunload>ClearProperty(DialogVisibility5,Home)</onunload>
	
	<controls>
		
		<control type="group">
			<visible>[String.Contains(Window(Home).Property(DialogVisibility),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility),$NUMBER[1]) + String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] | 
					[String.Contains(Window(Home).Property(DialogVisibility2),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility2),$NUMBER[2]) + String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility3),AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility3),$NUMBER[3]) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility4)AddonSettings) + String.Contains(Window(Home).Property(DialogVisibility4),$NUMBER[4]) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility5),AddonSettings)  + String.Contains(Window(Home).Property(DialogVisibility5),$NUMBER[5])]</visible>
			
			<include content="DialogPOS">
				<param name="DialogName" value="DiaAddonSettings" />
			</include>

		</control>

		<control type="button" id="3">
			<width>588</width>
			<height>60</height>
			<font>Font27</font>
			<textwidth>540</textwidth>
			<textoffsetx>12</textoffsetx>
			<texturefocus colordiffuse="$VAR[CategoryColor3]">common/white.png</texturefocus>
			<texturenofocus colordiffuse="00FFFFFF">common/white.png</texturenofocus>
			<textcolor>$INFO[Skin.String(Color.DIATxtNF)]</textcolor>
			<focusedcolor>$INFO[Skin.String(Color.DIATxtF)]</focusedcolor>
			<disabledcolor>$INFO[Skin.String(Color.DIAInvalid)]</disabledcolor>
			<invalidcolor>$INFO[Skin.String(Color.DIAInvalid)]</invalidcolor>
			<onleft condition="ControlGroup(2).HasFocus()">9</onleft>
			<onright condition="ControlGroup(9).HasFocus()">2</onright>
			<onright condition="ControlGroup(2).HasFocus()">9000</onright>
			<visible>Control.IsEnabled(3)</visible>
