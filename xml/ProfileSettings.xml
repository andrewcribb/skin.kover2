<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- FileManager -->
	<backgroundcolor>0x00000000</backgroundcolor>
	<defaultcontrol always="true">5</defaultcontrol>
	<onload condition="String.IsEmpty(Window(Home).Property(DialogVisibility)) + String.IsEmpty(Window(Home).Property(DialogVisibility2))">SetProperty(DialogVisibility,$INFO[Window.Property(xmlfile)]1,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility)) + String.IsEmpty(Window(Home).Property(DialogVisibility2))">SetProperty(DialogVisibility2,$INFO[Window.Property(xmlfile)]2,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.IsEmpty(Window(Home).Property(DialogVisibility3))">SetProperty(DialogVisibility3,$INFO[Window.Property(xmlfile)]3,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4))">SetProperty(DialogVisibility4,$INFO[Window.Property(xmlfile)]4,Home)</onload>
	<onload condition="!String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))">SetProperty(DialogVisibility5,$INFO[Window.Property(xmlfile)]5,Home)</onload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.Contains(Window(Home).Property(DialogVisibility),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility),$NUMBER[1])">ClearProperty(DialogVisibility,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.Contains(Window(Home).Property(DialogVisibility2),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility2),$NUMBER[2])">ClearProperty(DialogVisibility2,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.Contains(Window(Home).Property(DialogVisibility3),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility3),$NUMBER[3])">ClearProperty(DialogVisibility3,Home)</onunload>
	<onunload condition="String.IsEmpty(Window(Home).Property(DialogVisibility5)) + String.Contains(Window(Home).Property(DialogVisibility4),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility4),$NUMBER[4])">ClearProperty(DialogVisibility4,Home)</onunload>
	<onunload>ClearProperty(DialogVisibility5,Home)</onunload>
	
	<controls>

		<!-- Decorative Elements -->
		<control type="group">
			<!-- Background -->
			<include content="Wallpaper">
				<param name="CompPrefix" value="HOM.View1" />
			</include>

			<!-- Background -->
			<control type="image">
				<aspectratio>scale</aspectratio>
				<texture colordiffuse="$INFO[Skin.String(Color.BBBg)]">common/white.png</texture>
			</control>
			
			<!-- Left Panel Background -->
			<control type="image">
				<top>0</top>
				<left>0</left>
				<width>660</width>
				<height>1080</height>
				<aspectratio>scale</aspectratio>
				<texture colordiffuse="$INFO[Skin.String(Color.DIABg)]">common/white.png</texture>
			</control>
		</control>
		
		<control type="group">
			<visible>[String.Contains(Window(Home).Property(DialogVisibility),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility),$NUMBER[1]) + String.IsEmpty(Window(Home).Property(DialogVisibility2)) + String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] | 
					[String.Contains(Window(Home).Property(DialogVisibility2),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility2),$NUMBER[2]) + String.IsEmpty(Window(Home).Property(DialogVisibility3)) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility3),ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility3),$NUMBER[3]) + String.IsEmpty(Window(Home).Property(DialogVisibility4)) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility4)ProfileSettings) + String.Contains(Window(Home).Property(DialogVisibility4),$NUMBER[4]) + String.IsEmpty(Window(Home).Property(DialogVisibility5))] |
					[String.Contains(Window(Home).Property(DialogVisibility5),ProfileSettings)  + String.Contains(Window(Home).Property(DialogVisibility5),$NUMBER[5])]</visible>
			
			<include content="DialogPOS">
				<param name="DialogName" value="DIAProfileSettings" />
			</include>		

		</control>

		<!-- Default button -->
		<include content="DiaGroupListButton">
			<param name="Id" value="7" />
			<param name="OnLeft" value="noop" />
			<param name="OnRight" value="9000" />
		</include>

		<!-- Default radiobutton -->
		<include content="DiaRadio">
			<param name="Id" value="8" />
			<param name="OnLeft" value="noop" />
			<param name="OnRight" value="9000" />
		</include>

		<!-- Default spincontrolex -->
		<include content="DiaSpin">
			<param name="Id" value="9" />
			<param name="OnLeft" value="noop" />
			<param name="OnRight" value="9000" />
		</include>

		<!-- Default edit -->
		<include content="DiaEdit">
			<param name="Id" value="12" />
			<param name="OnLeft" value="noop" />
			<param name="OnRight" value="9000" />
		</include>	

		<!-- Default Label -->
		<include content="DiaLabel">
			<param name="Id" value="14" />
		</include>

		<include>MainSettingsContent</include>		
			
	</controls>

</window>